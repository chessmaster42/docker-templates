<?xml version="1.0" encoding="utf-8"?>
<Containers>
    <TemplateURL>https://github.com/chessmaster42/docker-templates/blob/master/ldap-user-manager.xml</TemplateURL>
    <Icon>https://raw.githubusercontent.com/chessmaster42/docker-templates/master/img/ldap-user-manager.jpg</Icon>
    <Beta>False</Beta>
    <Category>Network:Other</Category>
    <Name>ldap-user-manager</Name>
    <Description> A PHP web-based interface for LDAP user account management and self-service password change.</Description>
    <Overview> A PHP web-based interface for LDAP user account management and self-service password change.</Overview>
    <Support>https://hub.docker.com/r/wheelybird/ldap-user-manager</Support>
    <Registry>https://hub.docker.com/r/wheelybird/ldap-user-manager/~/dockerfile/</Registry>
    <GitHub>https://github.com/wheelybird/ldap-user-manager</GitHub>
    <Repository>wheelybird/ldap-user-manager</Repository>
    <Project>https://github.com/wheelybird/ldap-user-manager</Project>
    <Privileged>false</Privileged>
    <Networking>
        <Mode>bridge</Mode>
        <Publish>
            <Port>
                <HostPort>2080</HostPort>
                <ContainerPort>80</ContainerPort>
                <Protocol>tcp</Protocol>
                <Description>Test description</Description>
            </Port>
            <Port>
                <HostPort>2443</HostPort>
                <ContainerPort>443</ContainerPort>
                <Protocol>tcp</Protocol>
                <Description>Another description</Description>
            </Port>
        </Publish>
    </Networking>
    <Environment>
        <Variable>
            <Value/>
            <Name>LDAP_URI</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value/>
            <Name>LDAP_BASE_DN</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value/>
            <Name>LDAP_ADMIN_BIND_DN</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value/>
            <Name>LDAP_ADMIN_BIND_PWD</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value/>
            <Name>LDAP_ADMINS_GROUP</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value/>
            <Name>SERVER_HOSTNAME</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value/>
            <Name>LDAP_USER_OU</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value/>
            <Name>LDAP_GROUP_OU</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value/>
            <Name>LDAP_GROUP_MEMBERSHIP_ATTRIBUTE</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value/>
            <Name>LDAP_GROUP_MEMBERSHIP_USES_UID</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value/>
            <Name>LDAP_ACCOUNT_ATTRIBUTE</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value/>
            <Name>LDAP_REQUIRE_STARTTLS</Name>
            <Mode/>
        </Variable>
        <Variable>
            <Value/>
            <Name>LDAP_TLS_CACERT</Name>
            <Mode/>
        </Variable>
    </Environment>
    <WebUI>http://[IP]:[PORT:2443]/</WebUI>
    <DonateText></DonateText>
    <DonateLink></DonateLink>
    <DonateImg></DonateImg>
    <ExtraParams></ExtraParams>
</Containers>
